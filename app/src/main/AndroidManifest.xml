<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhhl.ducha">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".app.Applicton">
        <activity android:name=".activity.MainActivity"
            android:configChanges="screenSize|keyboardHidden|orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--<activity android:name=".activity.MainActivity"/>-->
        <!--报表-->
        <activity android:name=".activity.ZDActivity.HomeReportActivity"
            android:configChanges="screenSize|keyboardHidden|orientation"/>
        <!--重点人员-->
        <activity android:name=".activity.ZDActivity.KeypersonnelActivity"/>
        <!--重点详情-->
        <activity android:name=".activity.ZDActivity.KeypersonnelActivityDetalis"
            android:launchMode="singleTop"/>
        <!--涉毒-->
        <activity android:name=".activity.BedrugActivity.BedActivity"/>
        <!--涉毒详情-->
        <activity android:name=".activity.BedrugActivity.BedActivitydetails"
            android:launchMode="singleTop"/>
        <!--涉毒报表-->
        <activity android:name=".activity.BedrugActivity.BedReportActivity"/>
    </application>

</manifest>